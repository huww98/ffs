_ffs()
{
    COMPREPLY=()
    local cur="${COMP_WORDS[COMP_CWORD]}"
    local prev="${COMP_WORDS[COMP_CWORD-1]}"
    local cmds="init help login whoami adduser mkdir cd pwd ls rm read write chmod"

    if [ $COMP_CWORD -eq 1 ]
    then
        compopt +o nospace
        COMPREPLY=( $(compgen -W "${cmds}" -- ${cur}) )
        return 0
    fi
}

complete -o nospace -F _ffs ffs
